<!doctype html>
<html>
  <head>
    <title>Thumbnails</title>
    <style>
      body{ text-align: center; }
      .container{ width: 960px; margin: 0 auto; }
      img{ width: 20%; display: block; float: left; }
    </style>
  </head>
  <body>
    Thumbnails
    <div class='container'>
      {{#each items }}
        <img src="{{ snippet.thumbnails.default.url }}" title="{{snippet.title}}">
      {{/each}}
    </div>
  </body>
</html>
